{
  "name": "jaamu-plus",
  "version": "1.0.0",
  "private": true,
  "description": "Jupyter as a markup, with extra formats",
  "author": "Simo Ryu",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "python3 compile_notebooks.py && gatsby develop --host 0.0.0.0 --port 8002",
    "start": "python3 compile_notebooks.py && gatsby develop",
    "build": "python3 compile_notebooks.py && gatsby build",
    "serve": "gatsby serve  --host 0.0.0.0 --port 8002",
    "clean": "gatsby clean",
    "deploy": "gatsby build"
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^2.0.2",
    "@chakra-ui/react": "^1.7.3",
    "@chakra-ui/theme-tools": "^1.3.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "framer-motion": "^4.1.17",
    "gatsby": "^4.4.0",
    "gatsby-plugin-google-analytics": "^4.4.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^2.4.0",
    "gatsby-plugin-mdx": "^3.4.0",
    "gatsby-plugin-react-helmet": "^5.7.0",
    "gatsby-plugin-sharp": "^4.4.0",
    "gatsby-source-filesystem": "^4.4.0",
    "gatsby-transformer-sharp": "^4.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.0",
    "react-share": "^4.4.0",
    "react-syntax-highlighter": "^15.4.5",
    "rehype-highlight": "^5.0.2",
    "rehype-katex": "^6.0.2",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "typescript": "^4.5.5"
  }
